{
    "extends": "../../tsconfig",
    "include": ["src"],
    "exclude": ["node_modules", "dist"],
    "compilerOptions":  {
        "baseUrl": "./",
        "paths": {
            "@axios/*": ["src/axios/*"],
            "@axios": ["src/axios/index.ts"],
            "@components": ["src/components"],
            "@models": ["src/models"],
            "@hooks": ["src/hooks"],
            "@utils": ["src/utils"],
            "@context/*": ["src/context/*"],
            "@context": ["src/context/index.ts"]
        }
    }
}
